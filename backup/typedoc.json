{
  "entryPoints": ["./src"],
  "entryPointStrategy": "expand",
  "out": "docs/api/typedoc",
  "theme": "default",
  "name": "NewTravel Commerce Plugin API",
  "includeVersion": true,
  "excludeExternals": true,
  "excludeNotDocumented": false,
  "excludePrivate": true,
  "excludeProtected": false,
  "excludeInternal": false,
  "readme": "./README.md",
  "plugin": [
    "typedoc-plugin-markdown"
  ],
  "gitRevision": "main",
  "validation": {
    "notExported": true,
    "invalidLink": true,
    "notDocumented": false
  },
  "categoryOrder": [
    "Controllers",
    "Services", 
    "Types",
    "Utilities",
    "Middleware",
    "*"
  ],
  "sort": [
    "source-order"
  ],
  "kindSortOrder": [
    "Document",
    "Project",
    "Module",
    "Namespace",
    "Enum",
    "EnumMember",
    "Class",
    "Interface",
    "TypeAlias",
    "Constructor",
    "Property",
    "Variable",
    "Function",
    "Accessor",
    "Method",
    "Parameter",
    "TypeParameter",
    "TypeLiteral",
    "CallSignature",
    "ConstructorSignature",
    "IndexSignature",
    "GetSignature",
    "SetSignature"
  ],
  "searchInComments": true,
  "searchInDocuments": true,
  "cleanOutputDir": true,
  "titleLink": "https://github.com/your-org/commerce-plugin",
  "navigationLinks": {
    "API Documentation": "http://localhost:3000/api-docs",
    "GitHub": "https://github.com/your-org/commerce-plugin",
    "Support": "mailto:api-support@newtravel.com"
  },
  "sidebarLinks": {
    "Getting Started": "https://github.com/your-org/commerce-plugin#quick-start",
    "Tutorials": "./tutorials/",
    "Examples": "./examples/"
  },
  "customCss": "./docs/assets/typedoc-custom.css",
  "gaID": "UA-XXXXXXXXX-X",
  "githubPages": true,
  "cname": "docs.newtravel.com",
  "hideGenerator": false,
  "visibilityFilters": {
    "protected": false,
    "private": false,
    "inherited": true,
    "external": false
  },
  "searchCategoryBoosts": {
    "Controllers": 1.5,
    "Services": 1.3,
    "Types": 1.1
  },
  "sourceLinkTemplate": "https://github.com/your-org/commerce-plugin/blob/{gitRevision}/{path}#L{line}",
  "exclude": [
    "**/*.test.ts",
    "**/*.spec.ts",
    "**/node_modules/**",
    "**/dist/**",
    "**/coverage/**",
    "**/tests/**"
  ],
  "externalPattern": [
    "**/node_modules/**"
  ],
  "excludeTags": [
    "@internal",
    "@alpha",
    "@beta"
  ],
  "stripInternal": true,
  "disableSources": false,
  "basePath": "./src",
  "includes": "./docs/includes",
  "media": "./docs/assets",
  "emit": "docs",
  "json": "docs/api/typedoc.json",
  "pretty": true,
  "preserveWatchOutput": false,
  "treatWarningsAsErrors": false,
  "intentionallyNotExported": [
    "InternalType",
    "PrivateInterface"
  ],
  "requiredToBeDocumented": [
    "Enum",
    "EnumMember",
    "Variable",
    "Function",
    "Class",
    "Interface",
    "Property",
    "Method",
    "Accessor",
    "TypeAlias"
  ]
}